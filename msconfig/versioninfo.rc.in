#include <windows.h>
VS_VERSION_INFO VERSIONINFO
FILEVERSION @CONDOR_MAJOR_VERSION@,@CONDOR_MINOR_VERSION@,@CONDOR_BUILD_NUMBER@
PRODUCTVERSION @CONDOR_MAJOR_VERSION@,@CONDOR_MINOR_VERSION@,@CONDOR_BUILD_NUMBER@
FILEFLAGSMASK VS_FFI_FILEFLAGSMASK
FILEFLAGS 0
FILEOS VOS__WINDOWS32
FILETYPE VFT_APP
FILESUBTYPE VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "FileVersion", "@CONDOR_MAJOR_VERSION@.@CONDOR_MINOR_VERSION@.@CONDOR_BUILD_NUMBER@\0"
            VALUE "ProductVersion", "@CONDOR_MAJOR_VERSION@.@CONDOR_MINOR_VERSION@.@CONDOR_BUILD_NUMBER@\0"
            VALUE "FileDescription", "@CONDOR_EXECUTABLE_NAME@ application"
            VALUE "InternalName", "@CONDOR_EXECUTABLE_NAME@"
            VALUE "LegalCopyright", "Copyright (C) 2015"
            VALUE "OriginalFilename", "@CONDOR_EXECUTABLE_NAME@.exe"
            VALUE "ProductName", "@CONDOR_EXECUTABLE_NAME@ application"
            VALUE "CompanyName", "High Throughput Computing (HTC)"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END