use CondorTest;

my $cmd = 'job_core_output_van.cmd';
my $testdesc =  'condor_submit output works - vanilla U';
my $testname = "job_core_output_van";
my $debuglevel = 2;

my @expected_output =
(
'first',
'second',
'third',
'fourth',
);

CondorTest::SetExpected(\@expected_output);

print "Setting expected output - ";

$ExitSuccess = sub {
	my %info = @_;

	my $output = $info{"output"};
	open(IN, "<$output" ) || die "bad\nCan't open $output: $!\n";
};

CondorTest::RegisterExitedSuccess( $testname, \&CondorTest::DefaultOutputTest );

if( CondorTest::RunTest($testname, $cmd, 0) ) {
	print "ok\n";
	CondorTest::debug("$testname: SUCCESS\n",$debuglevel);
	exit(0);
} else {
	print "bad\n";
	die "$testname: CondorTest::RunTest() failed\n";
}

